<!DOCTYPE html>
<html lang="en">
<head>
      <meta charset="UTF-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title>Document</title>
</head>
<body>
      <table class="table table-light">

            <tr>
                  <td>STT</td>
                  <td>Name</td>
                  <td>SDT</td>
                  <td>CMND</td>
            </tr>
            

            <% users.forEach(function(item){ %>
                  
               
            <tr>
                  <td><%= item.id %><td>
                  <td><%= item.name %></td>
                  <td><%= item.cmnd %></td>
                  <td><%= item.sdt %></td>
            </tr>
            
            <% }); %>
      </table>
</body>
</html>